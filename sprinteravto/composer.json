{
    "name": "studio32x32/layout/sprinteravto",
    "description": "sprinteravto layout project",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vlad Fedorov",
            "email": "gkorchun@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bower-asset/jquery": "3.x",
		"oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/magnific-popup": "1.1.0",
        "bower-asset/swiper": "dev-master",
        "bower-asset/ion.rangeslider": "2.2.0",
        "bower-asset/select2": "4.0.5",
        "bower-asset/perfect-scrollbar": "0.8.1"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "npm",
            "bower-asset-library": "bower"
        },
        "installer-types": ["bower-asset"],
        "installer-paths": {
            "bower/{$name}/": ["type:bower-asset"]
        }
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
