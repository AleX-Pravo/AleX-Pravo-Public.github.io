//= Navigation =
//- (source/scss/layout/_navigation.scss)
nav.navigation
    if (header)
        ul
            each val in Menu1

                if (val.name === nameTitle)
                    li.active
                        a(href=val.href)= val.name
                else
                    li
                        a(href=val.href)= val.name

        ul
            each val in Menu2

                if (val.name === nameTitle)
                    li.active
                        a(href=val.href)= val.name
                else
                    li
                        a(href=val.href)= val.name
    else
        ul
            each val in MenuMobile

                if (val.name === nameTitle)
                    li.active
                        a(href=val.href)= val.name
                else
                    li
                        a(href=val.href)= val.name
//= End navigation =
= '\n'
