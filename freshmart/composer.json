{
    "name": "layout-modules/freshmart",
    "description": "Freshmart layout project",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Kozachuk",
            "email": "css3@ukr.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bower-asset/jquery": "2.x",
        "bower-asset/normalize.css": "^5.0",
        "bower-asset/bxslider-4": "^4.2",
        "bower-asset/jquery-selectric": "^1.11",
        "bower-asset/slick-carousel": "^1.6",
        "oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/perfect-scrollbar": "dev-master",
        "bower-asset/select2": "4.0.7",
        "bower-asset/magnific-popup": "1.1.0",
        "bower-asset/air-datepicker": "2.2.3"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "npm",
            "bower-asset-library": "bower"
        },
        "installer-types": ["bower-asset"],
        "installer-paths": {
            "bower_components/{$name}/": ["type:bower-asset"]
        }
    },
     "config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
