{
    "name": "studio32x32/layout/mirad",
    "description": "mirad",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Datsiuk",
            "email": "mastercrutor89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bower-asset/jquery": "2.x",
        "bower-asset/slick-carousel": "dev-master",
        "oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/ion.rangeslider": "2.3.0",
        "bower-asset/magnific-popup": "1.1.0",
        "bower-asset/select2": "4.0",
        "bower-asset/perfect-scrollbar": "dev-master"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "npm",
            "bower-asset-library": "bower"
        },
        "installer-types": ["bower-asset"],
        "installer-paths": {
            "bower/{$name}/": ["type:bower-asset"]
        }
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [{
        "type": "composer",
        "url": "https://asset-packagist.org"
    }]
}
