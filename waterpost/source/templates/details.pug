//- Utility

include utils/_variable.pug

-
    pageTitle = 'details'
    headerInner = true
    anchor = true
    details = true
    rating = true
    searchList = true


    breadcrumbItems = breadcrumbDetailsItems

doctype html
html
    include layout/_head.pug

    body

        = '\n'
        //= Layout =
        #layout.layout
            include layout/_header.pug

            //= Main =
            main#main.main
                .container
                    .layout__box
                        include module/_breadcrumb.pug
                        include module/_provider.pug

            //= End main =
            = '\n'

            #shadow

        //= End layout =

        include layout/_footer.pug
        include module/_mobile-menu.pug
        include layout/_sprite.pug
        include layout/_scripts.pug
