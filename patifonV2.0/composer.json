{
    "name": "studio32x32/layout/patifonv2",
    "description": "Patifon v2.0 layout project",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Krasinskiy",
            "email": "rk@32x32.com.ua"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bower-asset/jquery": "^3.2",
        "bower-asset/slick-carousel": "dev-master",
        "bower-asset/owl.carousel": "dev-master",
        "bower-asset/magnific-popup": "dev-master",
        "bower-asset/select2": "dev-master",
        "oomphinc/composer-installers-extender": "dev-master",
        "bower-asset/croppie": "dev-master",
        "bower-asset/ionrangeslider": "2.2.0",
        "bower-asset/scrollmagic": "dev-master",
        "bower-asset/greensock": "2.1.2",
        "bower-asset/perfect-scrollbar": "0.8.1"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "npm",
            "bower-asset-library": "bower"
        },
		"installer-types": ["bower-asset"],
		"installer-paths": {
            "bower/{$name}/": ["type:bower-asset"]
        }
    },
	"config": {
		"fxp-asset": {
			"enabled": false
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
