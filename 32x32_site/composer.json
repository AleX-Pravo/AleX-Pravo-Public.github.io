{
    "name": "studio32x32/layout/32x32",
    "description": "32x32",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "ALex Pravo",
            "email": "ss911.varan@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bower-asset/jquery": "3.x",
        "oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/magnific-popup": "^1.1",
        "bower-asset/swiper": "dev-master",
        "bower-asset/fullpage.js": "dev-master",
        "bower-asset/scrollify": "dev-master",
        "bower-asset/scrollmagic": "dev-master",
        "bower-asset/greensock": "1.18.0"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "npm",
            "bower-asset-library": "bower"
        },
        "installer-types": ["bower-asset"],
        "installer-paths": {
            "bower/{$name}/": ["type:bower-asset"]
        }
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
