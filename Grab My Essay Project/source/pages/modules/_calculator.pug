// calculator
section.calculator
    .container
        .calculator__wrapper
            .calculator__caption
                h2.calculator__title Brief and to the Point
                h6.calculator__subtitle The best essay writing company on the market
                .calculator__img(style='background: url(../../assets/img/calculator.png) no-repeat top right;')
            form.calculator__form
                .calculator__box
                    .calculator__checkbox
                        input.calculator__input(type='checkbox' id='new_member_discount' checked='checked')
                        label.calculator__check(for='new_member_discount')
                        label.calculator__text(for='new_member_discount') Iâ€™m a new customer <span>(15% OFF)</span>
                .calculator__box
                    .calculator__field.f_group.currency
                        label.calculator__label Select currency
                        .calculator__item.currency_check.clearfix
                            input(type='radio' name='currency' id='currency_USD' checked='checked')
                            label(for='currency_USD') USD
                            input(type='radio' name='currency' id='currency_GBP')
                            label(for='currency_GBP') GBP
                            input(type='radio' name='currency' id='currency_EUR')
                            label(for='currency_EUR') EUR
                            input(type='radio' name='currency' id='currency_CAD')
                            label(for='currency_CAD') CAD
                            input(type='radio' name='currency' id='currency_AUD')
                            label(for='currency_AUD') AUD
                    .calculator__field.f_group
                        label.calculator__label.f_label Type of paper
                        .calculator__item.f_select
                            select
                                option Essay
                                option Essay
                    .calculator__field.f_group
                        label.calculator__label.f_label Type of service
                        .calculator__item.f_select
