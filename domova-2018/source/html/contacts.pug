-
    pageTitle = 'Контакти'
//- Utility
include utils/_variable.pug

doctype html
html
    include layout/_head.pug

    body

        = '\n'
        //= Layout =
        #layout
            include layout/_header.pug

            //= Main =
            main#main.main
                - var breadcrumbs = ['Головна сторінка', 'Контакти']
                include module/_breadcrumbs.pug

                .container
                    - var title = [{modifier: '', elem: 'h1', icon: '', text: 'Де нас шукати', name: 'Контакти'}]
                    include module/_title.pug

                    include module/_contacts.pug

            //= End main =
            = '\n'

        //= End layout =

        include layout/_footer.pug
        include layout/_scripts.pug
        script(src='assets/js/jquery.selectric' + minSuffix + '.js')
        script.
            $(document).ready(function () {
                $('.languagelist, .citylist').selectric();
            });
