//= contacts-list =
//- (source/assets/css/module/_contacts.scss)
ul.contacts__list
    each item in contacts
        li.contacts__list-item(class= item.class)
            +icon('icon-' + item.icon)
            .contacts__list-item__inner
                p= item.name
                a(href= item.href)= item.numbers
//= End contacts-list =
